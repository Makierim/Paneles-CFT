                @Html.DisplayFor(modelItem => item.NombreComponenbte)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoComponente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MarcaComponente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModeloComponente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
